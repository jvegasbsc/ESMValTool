        <launcher_arguments>               						[('execute_as_shell', False)]             </launcher_arguments>
		<tags> 													R_Land					  </tags>
	
		<model> OBS        ERA5-ta				   reanalysis				L4 	 2000 2001 @{OBSPATH}  </model>
